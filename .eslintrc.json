{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-underscore-dangle": "off",
    "no-console": "off",
    "consistent-return": "off",
    "no-unused-vars": "off",
    "indent": "off",
    "max-len": "off",
    "import/extensions": "off",
    "prefer-destructuring": "off"
  }
}
